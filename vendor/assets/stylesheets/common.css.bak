.note-single				{ cursor:-webkit-zoom-in; }
.note-single:hover .btn-note{ display:inline-block; }
/*text-overflow: ellipsis; overflow:hidden; white-space: nowrap;*/
/*.note-header				{ position:relative; padding:10px; font-weight:bold; color:#fff; height:40px; overflow:hidden; word-wrap:break-word; }
.note-header .note-name		{  }
.note-body					{ position:relative; clear:both; height:40px; padding:10px 0; }
